package ParteMario;
import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;
public class exportarPDFEquip implements ActionListener{
	private ListadoEquipMant listEq;
	private PDFEquipamiento pdf;
	public exportarPDFEquip(ListadoEquipMant list) {
	listEq = list;
	}
	@Override
	public void actionPerformed(ActionEvent e) {
		// TODO Auto-generated method stub
		switch(e.getActionCommand()) {
		case "exportar":
			
			pdf = new PDFEquipamiento();
			pdf.setBounds(200, 200, 600, 200);
			pdf.setVisible(true);
			
			break;
		}
	}

}
