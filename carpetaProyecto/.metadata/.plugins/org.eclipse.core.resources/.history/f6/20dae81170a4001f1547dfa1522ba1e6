package ParteMario;
import java.awt.GridLayout;
import javax.swing.JPanel;
import javax.swing.*;
public class ladoizq extends JPanel{
	private JPanel p, p1, p2, p3;
	private JLabel NomId, NomNombre, NomDescrip;
	private JTextField TxId, TxNom, TxDes;
	ladoizq(){
		p = new JPanel(new GridLayout(3,1));
		
		p1 = new JPanel();
		NomId = new JLabel();
		
		NomNombre = new JLabel();
		
		NomDescrip = new JLabel();
		
		
		p2 = new JPanel();
		
		p3 = new JPanel();
		
		p.add(p1);
		p.add(p2);
		p.add(p3);
		this.add(p);
	}	
}