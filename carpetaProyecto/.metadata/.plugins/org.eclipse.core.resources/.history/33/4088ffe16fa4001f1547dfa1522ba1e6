package ParteMario;
import java.awt.GridLayout;
import javax.swing.JPanel;
public class ladoizq extends JPanel{
	private JPanel p, p1, p2, p3;
	ladoizq(){
		p = new JPanel(new GridLayout(3,2));
		
		p1 = new JPanel();
		
		p2 = new JPanel();
		
		p3 = new JPanel();
		
		p.add(p1);
		p.add(p2);
		p.add(p3);
		this.add(p);
	}	
}