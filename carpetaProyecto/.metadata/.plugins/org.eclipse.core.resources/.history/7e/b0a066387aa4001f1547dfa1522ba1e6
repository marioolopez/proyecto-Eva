package ParteMario;
import javax.swing.JPanel;
import java.awt.GridLayout;
import javax.swing.*;
public class latderch extends JPanel{
	private JPanel p, p1, p2;
	latderch(){
		p = new JPanel(new GridLayout(2,1));

		p1 = new JPanel();
		
		p2 = new JPanel();
		
		
		p.add(p1);
		p.add(p2);
		this.add(p);
	}
	
}
